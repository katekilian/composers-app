<%= form_for @composer do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :birth_year %>
  <%= f.number_field :birth_year %>
  <br>
  <%= f.label :death_year %>
  <%= f.number_field :death_year %>
  <br>
  <%= f.label :era %>
  <%= f.text_field :era %>
  <br>
  <%= f.submit %>
<% end %>
